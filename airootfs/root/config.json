{
  "version": "2.5.5",
  "config_version": "2.5.5",
  "gfx_driver": "<GFXDRIVER>",
  "harddrives": ["/dev/<DISK>"],
  "hostname": "fmnx-<USER>",
  "keyboard-layout": "<KBLAYOUT>",
  "sys-language": "<SYSLANG>",
  "timezone": "<TIMEZONE>",
  "additional-repositories": [],
  "audio": "pipewire",
  "debug": false,
  "desktop-environment": "gnome",
  "bootloader": "systemd-bootctl",
  "script": "guided",
  "silent": false,
  "swap": true,
  "offline": false,
  "no_pkg_lookups": false,
  "sys-encoding": "UTF-8",
  "services": ["docker", "bluetooth"],
  "kernels": ["linux-zen"],
  "nic": {
    "dhcp": true,
    "dns": null,
    "gateway": null,
    "iface": null,
    "ip": null,
    "type": "nm"
  },
  "profile": {
    "path": "/usr/lib/python*/site-packages/archinstall/profiles/desktop.py"
  },
  "packages": [
    "git",
    "pack",
    "bluez",
    "bluez-utils",
    "dconf",
    "dconf-editor",
    "chromium",
    "nautilus",
    "xorg-drivers",
    "networkmanager",
    "gedit",
    "eog",
    "nano",
    "gnome-disk-utility",
    "gnome-keyring",
    "gnome-logs",
    "gnome-system-monitor",
    "gnome-shell-extensions",
    "gnome-tweaks",
    "gnome-characters",
    "gnome-font-viewer",
    "gnome-calculator",
    "gnome-calendar",
    "gnome-clocks",
    "gnome-shell-extension-dash-to-dock-git",
    "geary",
    "deja-dup",
    "baobab",
    "xdg-user-dirs-gtk",
    "papirus-icon-theme",
    "ttf-droid",
    "adw-gtk-theme",
    "zsh-autosuggestions",
    "zsh-syntax-highlighting",
    "zsh-theme-powerlevel10k-git",
    "docker",
    "docker-compose",
    "go",
    "flutter",
    "hugo-git"
  ],
  "mirror-region": {
    "Russia": {
      "http://archive-ru-mos.gaab-networks.de/arch/$repo/os/$arch": true,
      "http://mirror.kamtv.ru/archlinux/$repo/os/$arch": true,
      "http://mirror.nw-sys.ru/archlinux/$repo/os/$arch": true,
      "http://mirror.surf/archlinux/$repo/os/$arch": true,
      "http://mirror.truenetwork.ru/archlinux/$repo/os/$arch": true,
      "http://mirror.yandex.ru/archlinux/$repo/os/$arch": true,
      "http://mirrors.powernet.com.ru/archlinux/$repo/os/$arch": true,
      "https://archive-ru-mos.gaab-networks.de/arch/$repo/os/$arch": true,
      "https://mirror.kamtv.ru/archlinux/$repo/os/$arch": true,
      "https://mirror.nw-sys.ru/archlinux/$repo/os/$arch": true,
      "https://mirror.surf/archlinux/$repo/os/$arch": true,
      "https://mirror.truenetwork.ru/archlinux/$repo/os/$arch": true,
      "https://mirror.yal.sl-chat.ru/archlinux/$repo/os/$arch": true,
      "https://mirror.yandex.ru/archlinux/$repo/os/$arch": true
    }
  }
}
